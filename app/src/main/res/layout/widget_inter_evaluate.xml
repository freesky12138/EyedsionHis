<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:id="@+id/widget_delete_dialog_linear"
    android:layout_width="592px"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/common_white"
    android:orientation="vertical">

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:background="@color/main_num_color"
        android:orientation="vertical"
        >

        <TextView
            android:id="@+id/dialog_delete_title"
            android:layout_width="match_parent"
            android:layout_height="100px"
            android:gravity="center_vertical"
            android:layout_marginLeft="24px"
            android:textColor="@color/common_white"
            android:textSize="36px"
            />

    </com.zhy.autolayout.AutoLinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/main_num_color"
        />

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="60px"
        android:orientation="horizontal"
        android:layout_marginTop="24px"
        >
        <TextView
            android:id="@+id/inter_doctor_time1"
            android:layout_width="150px"
            android:layout_height="60px"
            android:textSize="24px"
            android:layout_marginLeft="100px"
            android:background="@drawable/heart_bg_green_button_no_green"
            android:textColor="@color/main_num_color"
            android:text="50豆"
            android:gravity="center"
            />
        <TextView
            android:id="@+id/inter_doctor_time2"
            android:layout_width="150px"
            android:layout_height="60px"
            android:textSize="24px"
            android:layout_marginLeft="92px"
            android:background="@drawable/heart_bg_green_button_no_green"
            android:textColor="@color/main_num_color"
            android:text="100豆"
            android:gravity="center"
            />

    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="84px"
        android:orientation="horizontal"
        android:layout_marginTop="24px"

        >
        <TextView
            android:id="@+id/inter_doctor_time3"
            android:layout_width="150px"
            android:layout_height="60px"
            android:textSize="24px"
            android:layout_marginLeft="100px"
            android:background="@drawable/heart_bg_green_button_no_green"
            android:textColor="@color/main_num_color"
            android:text="150豆"
            android:gravity="center"
            />
        <TextView
            android:id="@+id/inter_doctor_time4"
            android:layout_width="150px"
            android:layout_height="60px"
            android:textSize="24px"
            android:layout_marginLeft="92px"
            android:background="@drawable/heart_bg_green_button_no_green"
            android:textColor="@color/main_num_color"
            android:text="200豆"
            android:gravity="center"
            />

    </com.zhy.autolayout.AutoLinearLayout>

</com.zhy.autolayout.AutoLinearLayout>