<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAccent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context="eyedsion.soft.eyedsionhis.activity.MainActivity">


    <TextView
        android:id="@+id/hellow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!" />

    <Button
        android:id="@+id/get_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="data" />

    <Button
        android:id="@+id/ima_sub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上传图片" />

    <Button
        android:id="@+id/take"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拍照" />

    <Button
        android:id="@+id/move"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打开相册"
        />

    <Button
        android:id="@+id/to_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转列表"
        />

    <Button
        android:id="@+id/show_delete_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="删除Dialog"
        />

    <Button
        android:id="@+id/show_choose2_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="两个选项Dialog"
        />

    <Button
        android:id="@+id/show_choose2_desc_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="两个选项带描述Dialog"
        />

    <Button
        android:id="@+id/show_choose_only_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="单一选项Dialog"
        />

    <Button
        android:id="@+id/show_edit_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="编辑Dialo"
        />

    <Button
        android:id="@+id/show_choose_value_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="asdfasdf"
        />


</com.zhy.autolayout.AutoLinearLayout>
